{
	"list_title":"List of books",
	"file_title":"Book: $NAME",

	"list":{
		// files list parameter - see https://developers.google.com/drive/v3/reference/files/list		
		"q": "mimeType='application/vnd.google-apps.document' and '0B7cabAnVcz8xTHNUd1JieUFjQm8' in parents",
		// ^ must include mimeType='application/vnd.google-apps.document' because google docs is the only supported format by this reader
		"orderBy":"recency",
		"pageSize":1000,
		"fields":"files(createdTime,description,id,modifiedTime,name,properties,starred),kind,nextPageToken",
	}
}